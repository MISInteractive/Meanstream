<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <title></title>
    <link href="/css/app.css" rel="stylesheet">
    @Styles.Render("~/Content/css")
    <script src="/scripts/holder.js"></script>
    <script src="/scripts/jquery.js"></script>
    <script src="/scripts/dust.js"></script>
    <script src="/scripts/templates.js"></script>
    <script src="/scripts/ellipsis.js"></script>
    <script src="/scripts/app.js"></script>
</head>
<body>
    <div data-role="container" class="ui-sticky-footer">
        @RenderBody()
        <footer data-role="footer">
            <div class="row">
                <div class="columns default-12">
                    <hr>
                    <div class="ui-flex-container copyright">
                        <span>
                            Copyright @DateTime.Now.Year Somerset Bay Home
                        </span>
                    </div>
                </div>
            </div>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>


